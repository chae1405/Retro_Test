<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/button_get"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Get"
        app:layout_constraintBottom_toTopOf="@+id/Get_Paren"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="151dp" />

    <Button
        android:id="@+id/Get_Paren"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Get_Paren"
        app:layout_constraintBottom_toTopOf="@+id/Button_Post"
        app:layout_constraintTop_toBottomOf="@+id/button_get"
        tools:layout_editor_absoluteX="152dp" />

    <Button
        android:id="@+id/Button_Post"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Post"
        app:layout_constraintBottom_toTopOf="@+id/Update"
        app:layout_constraintTop_toBottomOf="@+id/Get_Paren"
        tools:layout_editor_absoluteX="158dp" />

    <Button
        android:id="@+id/Update"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Button"
        app:layout_constraintBottom_toTopOf="@+id/button5"
        app:layout_constraintTop_toBottomOf="@+id/Button_Post"
        tools:layout_editor_absoluteX="162dp" />

    <Button
        android:id="@+id/button5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Button"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Update"
        tools:layout_editor_absoluteX="162dp" />

</androidx.constraintlayout.widget.ConstraintLayout>